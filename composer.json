{
  "name": "code16/cookie-consent",
  "description": "Cookie consent bar & manage modal",
  "authors": [{
    "name": "Philippe Lonchampt",
    "email": "philippe@code16.fr"
  },
    {
      "name": "Antoine Guingand",
      "email": "antoine@code16.fr"
    }
  ],
  "require": {
    "php": "^8.0|^8.1|^8.2",
    "illuminate/support": "^8.0|^9.0|^10.0",
    "illuminate/view": "^8.0|^9.0|^10.0",
    "illuminate/cookie": "^8.0|^9.0|^10.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.19.0",
    "mockery/mockery": "^1.3.0",
    "phpunit/phpunit": "^9.5",
    "orchestra/testbench": "6.*|7.*|8.*"
  },
  "autoload": {
    "psr-4": {
      "Code16\\CookieConsent\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Code16\\CookieConsent\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Code16\\CookieConsent\\CookieConsentServiceProvider"
      ]
    }
  }
}
